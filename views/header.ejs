	<meta charset="utf-8">
	<title>Demo</title>
	<link rel="stylesheet" href="/assets/sementic/semantic.min.css">
	<style >
		.main.container{
			margin-top: 7em;
		}
	</style>
</head>
<header>
	
</header>
<body>
	<div class="ui fixed inverted menu">
		<a href="/" class="ui header item">Accueil</a>
	</div>
	<div class="ui main container">
		<% if (locals.flash && locals.flash.error) { %>
	<div class="ui negative message ">
		<%= flash.error %>
	</div>
		<% } %>
		<% if (locals.flash && locals.flash.success) { %>
	<div class="ui positive message ">
		<%= flash.success %>
	</div>
		<% } %>